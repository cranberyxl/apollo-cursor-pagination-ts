{
  "name": "apollo-cursor-pagination-ts",
  "version": "3.1.1",
  "description": "Relay's Connection implementation for Apollo Server GraphQL library with primary key support for cursor generation.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cranberyxl/apollo-cursor-pagination.git"
  },
  "keywords": [
    "apollo",
    "pagination",
    "graphql",
    "knex"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Daniel Merrill <daniel@terminal.co>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*\""
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.840.0",
    "@aws-sdk/lib-dynamodb": "^3.840.0",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.28.0",
    "@faker-js/faker": "^9.8.0",
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.0",
    "@swc/jest": "^0.2.38",
    "@types/jest": "^29.5.12",
    "@types/node": "^16.0.0",
    "@types/rosie": "^0.0.45",
    "dynamodb-toolbox": "^2.6.5",
    "eslint": "^9.28.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-airbnb-extended": "^1.0.11",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-import-x": "^4.15.1",
    "eslint-plugin-jest": "^21.25.0",
    "eslint-plugin-n": "^17.19.0",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "knex": "^3.1.0",
    "prettier": "^3.5.3",
    "rosie": "^2.1.1",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "peerDependencies": {
    "dynamodb-toolbox": "^2.6.5",
    "knex": "*"
  }
}
